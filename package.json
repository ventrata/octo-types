{
	"name": "@ventrata/octo-types",
	"version": "0.0.3",
	"private": false,
	"license": "ISC",
	"files": [
		"dist/index.d.ts",
		"dist/models/**/*.d.ts",
		"dist/schemas/**/*.d.ts"
	],
	"types": "dist/index.d.ts",
	"repository": {
		"url": "https://github.com/ventrata/octo-types"
	},
	"main": "dist/index.js",
	"scripts": {
		"get:openapi:yaml": "ts-node src/utils/get-openapi-yaml.ts",
		"clean": "rm -rf dist",
		"generate": "openapi-ts && rm -f src/models/index.ts",
		"build": "npx tsc",
		"build:dry": "tsc --noEmit",
		"build:dry:ci": "tsc --noEmit",
		"check": "npm run build:dry && npm run format && npm run lint",
		"fix": "biome check --write .",
		"format": "biome format",
		"format:fix": "biome format --write .",
		"lint": "biome lint",
		"lint:fix": "biome lint --write .",
		"audit": "npm audit",
		"test": "",
		"prepare": "npm run build"
	},
	"peerDependencies": {
		"zod": "4.1.3"
	},
	"dependencies": {
		"@biomejs/biome": "2.2.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2"
	},
	"devDependencies": {
		"@hey-api/openapi-ts": "0.81.1",
		"@types/node": "^22.9.0",
		"zod": "4.1.3"
	}
}
